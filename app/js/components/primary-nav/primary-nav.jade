//- account-select
nav.primary-nav(role="navigation", aria-label="Primary Navigation", a="true")
  ul
    li
      a(ui-sref='app.overview', ui-sref-active='current', md-ink-ripple) 
        i.fa.fa-tachometer
        span.text(translate) Overview
    li
      a(ui-sref='app.actions', ui-sref-active='current', md-ink-ripple) 
        i.fa.fa-file-text
        span.text(translate) Actions
    li
      a(ui-sref='app.inventory', ui-sref-active='current', md-ink-ripple) 
        i.fa.fa-database
        span.text(translate) Inventory
    li(ng-if='config.isPlannerEnabled')
      a(ui-sref='app.maintenance', ui-sref-active='current', md-ink-ripple) 
        i.fa.fa-wrench
        span.text(translate) Planner
    li
      a(ui-sref='app.rules', ng-class='{current: includes(states.rules, state.current.name)}', md-ink-ripple) 
        i.fa.fa-institution
        span.text(translate) Rules
    li
      a(ui-sref='app.digests', ui-sref-active='current', md-ink-ripple) 
        i.fa.fa-bar-chart
        span.text(translate) Executive Report
    li
      a(ui-sref='app.config', ui-sref-active='current', md-ink-ripple) 
        i.fa.fa-file-text
        span.text(translate) Configuration
